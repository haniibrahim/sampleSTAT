EXE = sampleSTAT
FC = gfortran
IDIR =
CFLAGS = -Wall -O2  -J$(OBJS_DIR) $(IDIR)
LFLAGS = -s
LIBS =

VPATH = $(SRC_DIR_mdd1):$(OBJS_DIR):$(SRC_DIR_f90d1):$(OBJS_DIR):$(SRC_DIR_F90d1):$(OBJS_DIR)
OBJS = $(addprefix $(OBJS_DIR), $(OBJS_mdd1) $(OBJS_f90d1) $(OBJS_F90d1))

all : $(EXE)

$(EXE) : $(OBJS_mdd1) $(OBJS_f90d1) $(OBJS_F90d1)
	@mkdir -p $(EXE_DIR)
	$(FC) -o $(EXE_DIR)$(EXE) $(OBJS) $(LFLAGS) $(LIBS)

$(OBJS_mdd1):
	@mkdir -p $(OBJS_DIR)
	$(FC) $(CFLAGS) -c $(SRC_DIR_mdd1)$(@:.o=.md) -o $(OBJS_DIR)$@

$(OBJS_f90d1):
	@mkdir -p $(OBJS_DIR)
	$(FC) $(CFLAGS) -c $(SRC_DIR_f90d1)$(@:.o=.f90) -o $(OBJS_DIR)$@

$(OBJS_F90d1):
	@mkdir -p $(OBJS_DIR)
	$(FC) $(CFLAGS) -c $(SRC_DIR_F90d1)$(@:.o=.F90) -o $(OBJS_DIR)$@

clean :
	rm -f $(OBJS_DIR)*.*
	rm -f $(EXE_DIR)$(EXE)

